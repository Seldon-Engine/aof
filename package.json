{
  "name": "aof",
  "version": "0.1.0",
  "description": "Agentic Ops Fabric â€” deterministic orchestration for multi-agent systems",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "bin": {
    "aof": "dist/cli/index.js",
    "aof-daemon": "dist/daemon/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "deploy:plugin": "scripts/deploy-plugin.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config tests/vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config tests/vitest.e2e.config.ts",
    "test:e2e:verbose": "VERBOSE_TESTS=true vitest run --config tests/vitest.e2e.config.ts",
    "test:integration:plugin": "vitest run --config tests/integration/vitest.config.ts",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "node dist/cli/index.js lint",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "aof",
    "agents",
    "orchestration",
    "deterministic",
    "openclaw"
  ],
  "author": "Xavier Spriet",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "better-sqlite3": "^12.6.2",
    "commander": "^14.0.3",
    "gray-matter": "^4.0.3",
    "prom-client": "^15.1.3",
    "write-file-atomic": "^7.0.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
